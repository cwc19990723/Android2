package com.example.day13_lxc3;

public class Carr {
	
	private int image;
	private String name;
	private String price;
	
	public Carr() {
	}

	public Carr(int image, String name, String price) {
		super();
		this.image = image;
		this.name = name;
		this.price = price;
	}

	public int getImage() {
		return image;
	}

	public void setImage(int image) {
		this.image = image;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "Car [image=" + image + ", name=" + name + ", price=" + price + "]";
	}
	
}
